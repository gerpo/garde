{
  "name": "garde",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "test:unit": "jest --verbose",
    "dev": "vite"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.4",
    "@popperjs/core": "^2.10.2",
    "@tailwindcss/forms": "^0.3.4",
    "@vueuse/core": "^6.5.3",
    "axios": "^0.23.0",
    "register-service-worker": "^1.7.1",
    "v-calendar": "^3.0.0-alpha.6",
    "vue": "^3.2.16",
    "vue-i18n": "9.2.0-beta.11",
    "vue-router": "^4.0.11",
    "vue-toastification": "^2.0.0-rc.2",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.2.2",
    "@types/istanbul-reports": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/test-utils": "^2.0.0-rc.16",
    "autoprefixer": "^10.3.7",
    "jest": "^27.2.5",
    "msw": "^0.35.0",
    "postcss": "^8.3.9",
    "tailwindcss": "^2.2.16",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-pwa": "^0.11.3",
    "vite-svg-loader": "^2.2.0",
    "vue-tsc": "^0.3.0",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
